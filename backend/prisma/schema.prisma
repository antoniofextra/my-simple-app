datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Todo {
  id        Int     @id @default(autoincrement())
  title     String
  location  String? // Max 20 characters
  completed Boolean @default(false)
  createdAt DateTime @default(now())
}
